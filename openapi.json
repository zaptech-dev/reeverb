{
  "components": {
    "schemas": {
      "ErrorResponse": {
        "properties": {
          "error": {
            "properties": {
              "code": {
                "description": "Machine-readable error code",
                "type": "string"
              },
              "details": {
                "additionalProperties": true,
                "description": "Optional additional details",
                "type": "object"
              },
              "message": {
                "description": "Human-readable error message",
                "type": "string"
              }
            },
            "required": [
              "code",
              "message"
            ],
            "type": "object"
          }
        },
        "required": [
          "error",
          "trace_id"
        ],
        "type": "object"
      }
    }
  },
  "info": {
    "title": "Reeverb API",
    "version": "0.1.0"
  },
  "openapi": "3.0.3",
  "paths": {
    "/api/v1/auth/login": {
      "post": {
        "operationId": "login",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$defs": {
                    "UserResponse": {
                      "properties": {
                        "avatar_url": {
                          "type": [
                            "string",
                            "null"
                          ]
                        },
                        "email": {
                          "type": "string"
                        },
                        "id": {
                          "type": "string"
                        },
                        "name": {
                          "type": [
                            "string",
                            "null"
                          ]
                        }
                      },
                      "required": [
                        "id",
                        "email"
                      ],
                      "type": "object"
                    }
                  },
                  "$schema": "https://json-schema.org/draft/2020-12/schema",
                  "properties": {
                    "expires_in": {
                      "format": "uint64",
                      "minimum": 0,
                      "type": "integer"
                    },
                    "token": {
                      "type": "string"
                    },
                    "user": {
                      "$ref": "#/$defs/UserResponse"
                    }
                  },
                  "required": [
                    "token",
                    "expires_in",
                    "user"
                  ],
                  "title": "AuthResponse",
                  "type": "object"
                }
              }
            },
            "description": "Success"
          },
          "401": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "Invalid email or password"
          },
          "409": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "Email already registered"
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "Internal server error"
          },
          "default": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "Error response"
          }
        },
        "summary": "Login"
      }
    },
